<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="script.module.audo-programs"
       name="audo-programs"
       version="0.0.9~alpha1"
       provider-name="lsellens">
  <requires>
    <import addon="xbmc.addon" version="12.0.0"/>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.module"
             library="resources" />
  <extension point="xbmc.addon.metadata">
    <summary>Core Programs for audo</summary>
    <description>This is a package of core programs for audo. Containes SABnzbd, NZBGet, HeadPhones, CouchPotato, and Sickbeard(sickrage branch).</description>
    <platform>linux</platform>
  </extension>
</addon>
